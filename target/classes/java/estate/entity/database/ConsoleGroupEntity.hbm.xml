<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.ConsoleGroupEntity" table="console_group" schema="" catalog="PropertySystem">
        <id name="id">
            <column name="cg_id" sql-type="int unsigned" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="45"/>
        </property>
        <property name="authorization">
            <column name="authorization" sql-type="varchar"/>
        </property>
        <property name="menu">
            <column name="menu" sql-type="text" length="65535"/>
        </property>
    </class>
</hibernate-mapping>